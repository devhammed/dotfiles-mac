{
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "git.confirmSync": false,
  "explorer.confirmDelete": false,
  "workbench.list.defaultFindMatchType": "fuzzy",
  "application.shellEnvironmentResolutionTimeout": 120,
  "git.enableSmartCommit": true,
  "workbench.iconTheme": "a-file-icon-vscode",
  "workbench.productIconTheme": "a-file-icon-vscode-product-icon-theme",
  "redhat.telemetry.enabled": true,
  "cursor.cpp.enablePartialAccepts": true,
  "explorer.compactFolders": false,
  "scm.compactFolders": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "notebook.insertFinalNewline": true,
  "debug.internalConsoleOptions": "openOnSessionStart",
  "database-client.autoSync": true,
  "files.associations": {
    "*.svg": "html"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "json.schemaDownload.enable": true,
  "cursor.cpp.disabledLanguages": ["plaintext"],
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#00634A",
    "tab.unfocusedActiveBorder": "#ff000000"
  },
  "workbench.colorTheme": "Dracula Theme",
  "files.trimTrailingWhitespace": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "githubPullRequests.queries": [
    {
      "label": "Waiting For My Review",
      "query": "is:open review-requested:${user}"
    },
    {
      "label": "Assigned To Me",
      "query": "is:open assignee:${user}"
    },
    {
      "label": "Created By Me",
      "query": "is:open author:${user}"
    },
    {
      "label": "All Merged",
      "query": "is:merged"
    }
  ],
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "database-client.capitalizeKeywordsWhenFormatting": true,
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": true,
  "cursor.terminal.usePreviewBox": true,
  "editor.fontFamily": "'JetBrains Mono', monospace",
  "editor.fontSize": 13,
  "editor.fontLigatures": true
}
